ENTITY manchester is
PORT (
	A, B, Cin : in bit;
	S, Cout : out bit
);
END manchester;
architecture comportamento of manchester is 
BEGIN 
	S <= A xor B xor Cin;
	Cout <= (A and b) or (Cin and (A xor B));
END comportamento;

